<UserControl x:Class="Digital_twin.UserControls.Search"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Digital_twin.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="300" x:Name="root">
    <DockPanel>
        <Button DockPanel.Dock="Right" Click="Browse">Browse OSM file</Button>
        <TextBox Margin="0,0,2,0" Text="{Binding ElementName=root, Path=FileName}" TextChanged="InputChanged" ></TextBox>
    </DockPanel>
</UserControl>
